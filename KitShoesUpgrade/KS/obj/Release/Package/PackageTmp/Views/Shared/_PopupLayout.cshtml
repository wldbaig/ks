@using Kendo.Mvc.UI
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    @*<title>@ViewBag.Title - Black Arrow</title>*@
    <title>@ViewBag.Title</title>
    <meta name="description" content="description">
    <meta name="author" content="Black Arrow">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @Styles.Render("~/bundles/css/bootstrap")

    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <link href='//fonts.googleapis.com/css?family=Righteous' rel='stylesheet' type='text/css'>
    @Styles.Render("~/bundles/mandatorystyles")

    @{

        <link rel="stylesheet" href="~/Content/Kendo/css/kendo.silver.min.css">

    }

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
            <script src="http://getbootstrap.com/docs-assets/js/html5shiv.js"></script>
            <script src="http://getbootstrap.com/docs-assets/js/respond.min.js"></script>
    <![endif]-->
    @Scripts.Render("~/bundles/jquerylib")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/mandatoryjs")
    @RenderSection("Styles", false)

</head>
<body style="background-color:#fff;">
    <!--Start Container-->
    <div id="main-" class="container-fluid">
        <div class="row">
            <!--Start Content-->
            <div id="content-" class="col-xs-12 col-sm-12">
                <div class="preloader">
                    <img src="~/Content/img/devoops/devoops_getdata.gif" class="devoops-getdata" alt="preloader" />
                </div>
                <div id="ajax-content">@RenderBody()</div>
            </div>
            <!--End Content-->
        </div>
    </div>
    <!--End Container-->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    @Scripts.Render("~/bundles/jqueryui")

    <!-- Bootstrap -->
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)

    <script src="http://kendo.cdn.telerik.com/2016.2.714/js/kendo.all.min.js"></script>
    @Scripts.Render("~/bundles/kendojs")
    <script type="text/javascript">
        
    </script>
</body>
</html>
